{
  "name": "react-atom-styled",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-earth/react-atom.git"
  },
  "author": "easilyjs",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "release": "release-it"
  },
  "dependencies": {
    "react-atom-core": "0.1.2"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "styled-components": ">=5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-node-externals": "^4.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "styled-components": "^5.0.0",
    "typescript": "^5.2.2"
  },
  "release-it": {
    "git": false,
    "plugins": {
      "@release-it/bumper": {
        "out": {
          "file": "package.json",
          "path": [
            "dependencies.react-atom-core"
          ]
        }
      }
    }
  }
}
